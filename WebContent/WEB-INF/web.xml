	</servlet>
	<servlet>
		<servlet-name>BoardDownloadServlet</servlet-name>
		<servlet-class>com.flow.controller.BoardDownloadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BoardPieChartServlet</servlet-name>
		<servlet-class>com.flow.controller.BoardPieChartServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>APIScheduler</servlet-name>
		<servlet-class>com.flow.quartz.servlet.APIScheduler</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>APIScheduler2</servlet-name>
		<servlet-class>com.flow.quartz.servlet.APIScheduler2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>APISchedulerCheck</servlet-name>
		<servlet-class>com.flow.quartz.servlet.APISchedulerCheck</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>APISchedulerCheck2</servlet-name>
		<servlet-class>com.flow.quartz.servlet.APISchedulerCheck2</servlet-class>
	</servlet>
	
