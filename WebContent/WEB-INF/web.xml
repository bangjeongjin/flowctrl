	</servlet>
	<servlet>
		<servlet-name>BoardUpdateServlet</servlet-name>
		<servlet-class>com.flow.controller.BoardUpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BoardSearchServlet</servlet-name>
